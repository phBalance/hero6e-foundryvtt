{{log "actor-sheet-effects-partial-item" effects}}
{{#if effects}}
    <tr class="activeEffects {{css}}" data-tooltip="{{localize title}}">
        <th></th>
        <th class="left">{{localize headerName}}</th>
        <th>Active</th>
        <th></th>
    </tr>

    {{#each effects as |effect key|}}
        <tr class="active-effects {{css}}" data-effect-id="{{effect._id}}" >
            <td style="min-width: 24px">
                <div class="item-image"><img src="{{effect.img}}" data-tooltip="{{effect.name}}" width="24"
                        height="24" /></div>
            </td>
            <td class="left">{{effect.nameExtended}}</td>
            <td>
                <input class="effect-toggle" data-action="toggle" type="checkbox" {{checked (not
                    effect.disabled)}} data-dtype="Boolean" data-action="toggle" />
            </td>
            <td class="right">
                <a class="item-control effect-edit" data-action="edit" title="Edit effect"><i
                        class="fas fa-edit"></i></a>
                <a class="item-control effect-delete" data-action="delete" title="Delete effect">
                    <i class="fas fa-trash"></i>
                </a>
            </td>
        </tr>
    {{/each}}
    
{{/if}}