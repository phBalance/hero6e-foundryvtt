<section
  class='tab movements {{tab.cssClass}}'
  data-group='primary'
  data-tab='movements'
>
    <section class="item-list-container">
        <div class="item-header">
            <div class="item-column item-description">{{localize "ActorSheet.Description"}}</div>
            <div class="item-column item-end">{{localize "ActorSheet.Levels"}}</div>
            <div class="item-column item-controls"></div>
        </div>

        <ol class="item-list unlist">
            {{#each items as |item|}}
                {{> (getModulePath 'actor/actor-sheet-v2-parts/actor-sheet-movements-partial-item-v2.hbs') item }}
            {{/each}}
        </ol>
    </section>
</section>